<div class="page-header">
  <h1>Teachers</h1>
</div>
<div class="table-responsive">
<table class="table table-striped table-hover table-bordered">
<thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Actions</th>
  </tr>
</thead>
<tbody>
  <% @teachers.each do |teacher| %>
  <tr>
    <td><%= teacher.first_name %></td>
    <td><%= link_to teacher.last_name, teacher %></td>
    <td><a href="#">Edit</a></td>
  </tr>
  <% end %>
</tbody>
</table>
</div>
